        android:scaleType="fitXY"
        android:id="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="4"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t04_madre_totemica"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:id="@+id/imageButton4"
        android:layout_below="@+id/imageButton2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="14dp" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="5"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t05_toro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:id="@+id/imageButton5"
        android:layout_alignTop="@+id/imageButton4"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="6"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t06_tatuje_abstracto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageButton6" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="7"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t07_aguila"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_marginTop="21dp"
        android:id="@+id/imageButton7"
