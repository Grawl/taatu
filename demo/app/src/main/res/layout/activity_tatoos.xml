        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_alignTop="@+id/imageButton8"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageButton9" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="10"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t10_ni_idea"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_marginTop="22dp"
        android:id="@+id/imageButton10"
        android:layout_below="@+id/imageButton7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="11"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t11_oso"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_alignTop="@+id/imageButton10"
        android:layout_alignLeft="@+id/imageButton8"
        android:layout_alignStart="@+id/imageButton8"
        android:id="@+id/imageButton11" />

    <ImageButton
        android:onClick="tattooClick"
        android:tag="12"
        android:maxWidth="300px"
        android:maxHeight="300px"
        android:src="@drawable/t12_sol"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:adjustViewBounds="true"
        android:cropToPadding="false"
        android:background="#00000000"
        android:scaleType="fitXY"
        android:layout_alignBottom="@+id/imageButton11"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/imageButton12" />


</RelativeLayout>
